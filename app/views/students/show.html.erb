<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @student.name %>
</p>

<p>
  <strong>Major</strong>
  <%= @student.major %>
</p>

<%= link_to 'Edit', edit_student_path(@student) %> |
<%= link_to 'Back', @student.school %>



<h3>Books</h3>
<% @student.books.find_each do |b| %>
    <p>
      <strong>Name:</strong>
      <%= b.title %>
    </p>

    <p>
      <strong>Author:</strong>
      <%= b.author %>
    </p>
<% end %>
