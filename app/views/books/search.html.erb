<div class="page-header">
  <h1>Books Search</h1>
  <h3><%= @books.count %> results found</h3>
</div>

  <tbody>
    <% @books.each do |b| %>
      <div class="panel panel-primary">
        <div class="panel-heading">
          <h3 class="panel-title">
            <%= b.title %>
          </h3>
          <h5> posted by <%= b.student.name %>
        </div>
        <div class="panel-body">
         <h4><strong>Author:</strong> <%= b.author %> </h4>
         <h5><strong>Price:</strong> $<%=  b.price %> </h4>
        </div>
      </div>
    <% end %>
  </tbody>
</table>

<br>
